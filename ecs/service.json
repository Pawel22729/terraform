[
    {
      "name": "hello_world",
      "image": "hello-world",
      "cpu": 512,
      "memory": 512,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "eu-west-2",
          "awslogs-group": "hello_world",
          "awslogs-stream-prefix": "hello-ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 80
        }
      ]
    }
  ]
  